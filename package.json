{
  "name": "homodynamics--by-amit-srivastava",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode production && open dist/stats.html",
    "build:production": "NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "size-check": "npm run build && ls -lah dist/assets/",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy:prepare": "./deploy.sh",
    "deploy:check": "npm run test && npm run build:production && npm run size-check"
  },
  "dependencies": {
    "d3": "7",
    "d3-array": "^3.2.4",
    "d3-drag": "^3.0.0",
    "d3-ease": "^3.0.1",
    "d3-force": "^3.0.0",
    "d3-path": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-timer": "^3.0.1",
    "d3-transition": "^3.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/d3": "^7.4.3",
    "@types/d3-array": "^3.2.1",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-force": "^3.0.9",
    "@types/d3-path": "^3.1.0",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-selection": "^3.0.10",
    "@types/d3-shape": "^3.1.6",
    "@types/d3-timer": "^3.0.2",
    "@types/d3-transition": "^3.0.8",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.3.4",
    "axe-core": "^4.10.2",
    "jsdom": "^25.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.44.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^2.1.8"
  }
